<h1>注文履歴一覧</h1>

 


<table class="table">
	<thead>
		<tr>
		  <th>購入日時</th>
			<th>購入者</th>
			<th>注文個数</th>
			<th>注文ステータス</th>
			
        </tr>
    </thead>
  

    <% @orders.each do |order| %>
      <tbody>
   
        <tr>
    	    <td><%= order.created_at %></td>
    	    <td><%= order.user %></td>
    	    <td><%= order.number %></td>
            <td><%= order.order_status %></td>
        </tr>
 
      </tbody>
  <% end %>
</table>
